<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/sign_in_pag"
        android:layout_width="360dp"
        android:layout_height="800dp"
        android:clipToOutline="true"
        android:background="#FFFFFF"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/group_1"
        android:layout_width="360dp"
        android:layout_height="225dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/rectangle_1"
        android:layout_width="360dp"
        android:layout_height="222dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="3dp"
        android:background="#FFFFFF"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/vector_1"
        android:layout_width="360dp"
        android:layout_height="188.1dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/vector_1"
        tools:ignore="MissingConstraints" />

    <!-- Auto layout, variables, and unit scale are not yet supported -->
    <View
        android:id="@+id/group_2"
        android:layout_width="285dp"
        android:layout_height="423dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="15dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="8dp"
        />

    <View
        android:id="@+id/rectangle_2"
        android:layout_width="241dp"
        android:layout_height="316dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="59dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="115dp"
        android:background="@drawable/rectangle_2"
        android:elevation="4dp"
        />

    <View
        android:id="@+id/rectangle_5"
        android:layout_width="108dp"
        android:layout_height="29dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="124dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="333dp"
        android:background="@drawable/rectangle_5"
        />

    <TextView
        android:id="@+id/sign_in"
        android:layout_width="172dp"
        android:layout_height="44dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="15dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="8dp"
        android:text="@string/sign_in"
        android:textAppearance="@style/sign_in"
        android:gravity="top"
        />
    <TextView
        android:id="@+id/username_"
        android:layout_width="97dp"
        android:layout_height="32dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="75dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="137dp"
        android:text="@string/username_"
        android:textAppearance="@style/username_"
        android:gravity="top"
        />

    <View
        android:id="@+id/rectangle_3"
        android:layout_width="213dp"
        android:layout_height="31dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="74dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="161dp"
        android:background="@drawable/rectangle_3"
        />
    <View
        android:id="@+id/rectangle_4"
        android:layout_width="213dp"
        android:layout_height="31dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="73dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="232dp"
        android:background="@drawable/rectangle_4"
        />

    <TextView
        android:id="@+id/log_in"
        android:layout_width="51dp"
        android:layout_height="18dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="155dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="339dp"
        android:text="@string/log_in"
        android:textAppearance="@style/log_in"
        android:gravity="top"
        />


    </androidx.constraintlayout.widget.ConstraintLayout>